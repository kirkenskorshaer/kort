<Window x:Class="WindowsClient.UI.CardForPrint"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WindowsClient.UI"
        mc:Ignorable="d"
        Title="Kort Til Print" Height="300" Width="600">
	<DockPanel HorizontalAlignment="Stretch" LastChildFill="False" Margin="0,0,0,0" VerticalAlignment="Stretch">
		<Menu DockPanel.Dock="Top">
			<MenuItem Header="Print" Click="MenuItem_Click"/>
		</Menu>
		<Grid>
			<Image x:Name="cardImage" HorizontalAlignment="Stretch" Margin="10,10,10,10" VerticalAlignment="Stretch"/>
		</Grid>
	</DockPanel>
</Window>
